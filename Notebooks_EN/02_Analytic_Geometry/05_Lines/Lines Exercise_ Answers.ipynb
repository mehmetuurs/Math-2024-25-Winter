      "source": [
        "# Example: Finding the equation of a line\n",
        "def line_equation(point1, point2):\n",
        "    x1, y1 = point1\n",
        "    x2, y2 = point2\n",
        "    slope = (y2 - y1) / (x2 - x1)\n",
        "    intercept = y1 - slope * x1\n",
        "    return f'y = {slope}x + {intercept}'\n",
        "\n",
        "line_equation((1, 2), (3, 4))"
      ]
    }
